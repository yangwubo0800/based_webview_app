    <!-- 注意如果url中含有 & 符号，请将 "&"转义字符&amp;代替 -->
    <first_page_url url="http://www.baidu.com"/>


    <!-- 按返回键需要退出程序的url界面 -->
    <home_urls    url="http://XXXXX" />


    <!-- 配置引导页界面的个数，范围目前限制在2到5页之间，各项目配置后一定要替换正确的对应图片 -->
    <guide_page_count count="4" />

    <!-- 配置是否需要引导页, 需要填1，不需要填0 -->
    <need_guide_page need="0" />
   

